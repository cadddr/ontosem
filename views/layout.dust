<html>
<head>
  <link rel="stylesheet" href="css/main.css">
</head>
<body class="{page}">

  <header class="container gutters-sm-all">
    <div class="col-1">
      <h6>Ontosem</h6>
    </div>
    <div class="col-4"></div>
    <div class="col-7">
      {+toolbar/}
    </div>
  </header>

  <div class="fixedcontent container gutters-sm-all">
    {+fixedcontent/}
  </div>

  <div class="scroll">
    <div class="container gutters-sm-all main">
      {+content/}
    </div>
  </div>

  {?debugging}
  <div class="container gutters-sm-all debugging">
    <div class="col-12">
      <h3>Debugging:</h3>
      {test}
    </div>
  </div>
  {/debugging}

  <div id="data-sync">{data}</div>
  <!-- Hacky way to get JSON objects exposed to the client
    while also rendering the templated dust.js -->


<script src="js/zepto.js"></script>
<script src="js/layout.js"></script>
{#clientscripts}
<script src="js/{script}"></script>
{/clientscripts}
<script>
</script>
</body>
</html>
