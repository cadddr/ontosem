<html>
	<head>
		<link rel="stylesheet" href="css/intermediate.css">
		<link rel="stylesheet" href="css/main.css">
	</head>
	<body>
		<div id="body">
			<div id="pageHeader">
				Intermediate results for: "{data.sentence}"
			</div>
		</div>
		

		{# data.words}

		<table id="{word}_ID" class="wordParse open">
			<thead>
				<tr>
					<td colspan="3">
						<div class="wordHeader noSelect">
							<span data-entity-id="{data.lexEntries[word].id}" parent-id="{data.lexEntries[word].parent}" class="highlightable">
							{word}
							</span>
						</div>
					</td>
				</tr>
				
				{? senses}
					<tr class="propertiesRow">
						<td colspan="3">
							<div class="lexiconResults">
								Senses: {senses}{? synonyms},   Synonyms: {synonyms}{/ synonyms}
							</div>
						</td>
					</tr>
				{/ senses}
			</thead>
			
			
			<tbody>
				<tr>
					<th>
						Status
					</th>
					<th>
						Word
					</th>
					<th>
						Reason
					</th>
				</tr>
				{# events}
					<tr>
						<td>
							<div class="status">
								{data.labels[status]}
							</div>
						</td>
						<td>
							<div class="word">
							
							{# words}
								<!--<span class="pair">
								</span>-->
									<span data-entity-id="{data.lexEntries[.[0]].id}" parent-id="{data.lexEntries[.[0]].parent}" class="highlightable">{.[0]}</span><!--
									-->{@eq key=type value="pair"}<!--
										--> & <span data-entity-id="{data.lexEntries[.[1]].id}" parent-id="{data.lexEntries[.[1]].parent}" class="highlightable">{.[1]}</span>
										<br />
									{/eq}
							{/ words}
							
							</div>
						</td>
						<td>
							<div class="reason">
								<ul>
									{# reasons}
										<li>
											{.}
										</li>
									{/ reasons}
								</ul>
							</div>
						</td>
					</tr>
				{/ events}
			</tbody>
		</table>
		<br />
		{/ data.words}
		
		<div id="TMRId">
			<b>TMR:</b>
			<br />
			<div class="col-12">
				<form>
					<input type="checkbox" checked="true" id="toggleOptional" value="toggleOptional">
					<label for="toggleOptional">Show Optional Attributes</label>
				</form>
			</div>
			<div class="rich-viewer">
			{#data.tmrData}
				{> "tmr" /}
			{/data.tmrData}
			</div>
			<div id="dataString">{dataString}</div>
		</div>
		
		<script src="js/zepto.js"></script>
		<script src="js/client.js"></script>
	</body>
</html>